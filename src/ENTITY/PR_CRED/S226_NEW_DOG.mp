        <Parameter shortName="P_KIND_CREDIT">
            <ViewClassId>KIND_CREDITS</ViewClassId>
            <ViewFilter>( {C_CLASSES_LIST} is null  or  instr( {C_CLASSES_LIST} , ';'||[V_CR_CLASS]||';'  ) &gt; 0 )&#13;
and ( {C_DEPART_CODE} is null or coalesce(instr([V_STRING_DEPARTS],';'||{C_DEPART_CODE}||';'),0) &gt;0) &#13;
and ( [V_ADD_CLASS] is null or instr({C_CLASSES_LIST} , ';'||[V_ADD_CLASS]||';' ) &gt; 0)&#13;
and &#13;
( &#13;
   ([V_PRJ_KIND_CRED] is null ) &#13;
or&#13;
   (   &#13;
       {&amp;TYPE_PROJECT_NAME} = [V_PRJ_KIND_CRED] &#13;
       or &#13;
          not exists (select * from $$$.VW_CRIT_KIND_CREDITS vid_cred where vid_cred.U_2 = [V_PRJ_KIND_CRED])&#13;
       or &#13;
          exists (select * from $$$.VW_CRIT_KIND_PROJECT kind_proj where kind_proj.ref3 =  {KIND_ID}  and kind_proj.ID = [V_PRJ_KIND_CRED]) &#13;
   )&#13;
)&#13;
and  (nvl( {C_ARCHIEVE} ,'0')='0')&#13;
and not exists (select * from $$$.VW_CRIT_LIST_COMPOSITE_OPER cmps where cmps.REF2= {KIND_ID} )</ViewFilter>
            <ViewShortName>VW_CRIT_KIND_CREDITS</ViewShortName>
        </Parameter>
        <Parameter shortName="V_CLIENT_REQ">
            <Bindings>|EQ V_CLIENT_REQ.1.2.CLIENT_V %PARAM%.V_CLIENT_REQ.1.1</Bindings>
        </Parameter>
        <Parameter shortName="P_DEPART">
            <ViewClassId>DEPART</ViewClassId>
            <ViewFilter>([P_FILIAL] IS NULL or {&amp;C_FCODE} = [P_FILIAL])</ViewFilter>
